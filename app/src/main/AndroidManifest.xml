<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.uwm.mydiabitiestracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddActivity"></activity>
        <activity android:name=".insertion.FoodInsertActivity"></activity>
        <activity android:name=".insertion.GlucoseInsertActivity"></activity>
        <activity android:name=".insertion.ExerciseActivity"></activity>
        <activity android:name=".insertion.PrescriptionActivity"></activity>
        <activity android:name=".insertion.UserActivity"></activity>
        <activity android:name=".ListMainRecords"></activity>
    </application>

</manifest>